#!/usr/bin/env ruby
$:.push File.expand_path("../../lib", __FILE__)

require 'cocoafind'

list = ["DenHeadless","DTTableViewManager","DenHeadless","DTCollectionViewManager","DenHeadless","DTRequestVerifier"]
response = Cocoafind::GitHubApi.new.repositories(list)

response.each do |mash|
    puts mash.stargazers_count
end